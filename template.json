{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_TutoVMClientAD4_name": {
            "defaultValue": "TutoVMClientAD4",
            "type": "String"
        },
        "disks_TutoVMClientAD4_OsDisk_1_599c09991e4f4b33a04759dfdcf5063a_externalid": {
            "defaultValue": "/subscriptions/922efab2-bf0e-4c64-bd4e-9175595a3181/resourceGroups/TUTORIALRESOURCES/providers/Microsoft.Compute/disks/TutoVMClientAD3_OsDisk_1_599c09991e4f4b33a04759dfdcf5063a",
            "type": "String"
        },
        "networkInterfaces_TutoVMClientAD4_externalid": {
            "defaultValue": "/subscriptions/922efab2-bf0e-4c64-bd4e-9175595a3181/resourceGroups/TutorialResources/providers/Microsoft.Network/networkInterfaces/TutoVMClientAD3",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2018-10-01",
            "name": "[parameters('virtualMachines_TutoVMClientAD4_name')]",
            "location": "eastus",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_DS1_v2"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2016-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "[concat(parameters('virtualMachines_TutoVMClientAD4_name'), '_OsDisk_1_599c09991e4f4b33a04759dfdcf5063a')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "Premium_LRS",
                            "id": "[parameters('')]"
                        },
                        "diskSizeGB": 127
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_TutoVMClientAD4_name')]",
                    "adminUsername": "admincis",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_TutoVMClientAD4_externalid')]"
                        }
                    ]
                }
            }
        },
        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "apiVersion": "2018-10-01",
            "name": "[concat(parameters('virtualMachines_TutoVMClientAD4_name'), '/myCustomScript')]",
            "location": "eastus",
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', parameters('virtualMachines_TutoVMClientAD4_name'))]"
            ],
            "properties": {
                "autoUpgradeMinorVersion": true,
                "settings": {
                    "fileUris": [
                        "https://raw.githubusercontent.com/maximejouan/PS/master/snmp.ps1",
                        "https://raw.githubusercontent.com/maximejouan/PS/master/telnet.ps1",
                        "https://raw.githubusercontent.com/maximejouan/PS/master/assistance.ps1"
                    ],
                    "commandToExecute": "powershell -ExecutionPolicy Unrestricted -file snmp.ps1 "
                },
                "publisher": "Microsoft.Compute",
                "type": "CustomScriptExtension",
                "typeHandlerVersion": "1.4",
                "protectedSettings": {}
            }
        }
    ]
}
